{
  "name": "node-project-template",
  "version": "1.0.0",
  "description": "A node project template",
  "license": "BSD-2-Clause",
  "private": true,
  "author": "terrestris GmbH & Co. KG <info@terrestris.de>",
  "contributors": [
    {
      "name": "Daniel Koch",
      "email": "koch@terrestris.de",
      "url": "https://github.com/dnlkoch"
    },
    {
      "name": "Marc Jansen",
      "email": "jansen@terrestris.de",
      "url": "https://github.com/marcjansen"
    }
  ],
  "keywords": [
    "gis",
    "webgis",
    "mapping",
    "cartography",
    "openlayers"
  ],
  "repository": {
    "type": "git",
    "url": "http://gitlab.intranet.terrestris.de/dkoch/node-project-template"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./build/* coverage/*",
    "pretest": "npm run lint",
    "test": "npm run clean && BABEL_ENV=coverage karma start karma.conf.js --single-run",
    "test:watch": "karma start karma.conf.js",
    "lint": "eslint --ext .js examples/** src/** spec/**",
    "lint:fix": "eslint --fix --ext .js examples/** src/** spec/**",
    "serve:examples": "webpack-dev-server --config webpack.examples.config.js --content-base ./tutorials/ --hot",
    "build:examples": "echo \"Error: Not implemented yet\" && exit 1",
    "build:docs": "jsdoc --tutorials ./tutorials/ --package ./package.json --readme ./README.md -c .jsdoc",
    "build:js": "webpack -p --progress --config webpack.config.js"
  },
  "dependencies": {
    "ol": "4.2.0"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-istanbul": "4.1.4",
    "babel-preset-es2015": "6.24.1",
    "eslint": "4.1.1",
    "eslint-plugin-html": "3.0.0",
    "expect.js": "0.3.1",
    "ink-docstrap": "1.3.0",
    "jsdoc": "3.4.3",
    "jsdoc-babel": "0.3.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "mocha": "3.4.2",
    "rimraf": "2.6.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0"
  }
}
