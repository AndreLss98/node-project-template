{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": "./src",
    "includePattern": ".js",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "extensions": ["js"]
  },
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "private": false,
    "tutorials": "./examples/"
  },
  "templates": {
    "systemName": "Sol",
    "includeDate": true,
    "navType": "vertical",
    "outputSourceFiles": true,
    "theme": "yeti",
    "linenums": true,
    "search": true,
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
